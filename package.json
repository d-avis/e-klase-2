  },
  "homepage": "https://e-klases-paligs.netlify.app",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/webextension-polyfill": "^0.10.0",
    "concurrently": "^8.2.0",
