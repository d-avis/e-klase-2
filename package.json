  "description": "Moderns dizains un vairāk funkcijas E-klasei",
  "main": "index.js",
  "scripts": {
    "dev:chromium": "gulp build:chromium && webpack -w --config webpack.dev.js",
    "dev:firefox": "gulp build:firefox && concurrently \"webpack -w --config webpack.dev.js\" \"web-ext run --source-dir ./dist/\"",
    "build:chromium": "gulp build:chromium && webpack --config webpack.prod.js && gulp package:chromium",
    "build:firefox": "gulp build:firefox && webpack --config webpack.prod.js && gulp package:firefox",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
